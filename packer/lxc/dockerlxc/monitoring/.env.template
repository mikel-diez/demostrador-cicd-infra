# Grafana configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# InfluxDB 2.x configuration
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=adminadmin12345  # Must be at least 8 characters for InfluxDB 2.x
INFLUXDB_ORG=proxmox
INFLUXDB_BUCKET=proxmox
INFLUXDB_TOKEN=my-super-secret-admin-token
INFLUXDB_URL=http://influxdb:8086

# Proxmox configuration
PROXMOX_HOST=proxmox.local
# User format should be username@realm (e.g., root@pam or monitoring@pve)
PROXMOX_USER=root@pam
# API Token ID that you created in Proxmox UI (e.g., monitoring)
PROXMOX_TOKEN_ID=your_token_id
# API Token Secret - the long string generated when you created the token
PROXMOX_TOKEN_SECRET=your_token_secret 