# Grafana configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# InfluxDB configuration
INFLUXDB_DB=proxmox
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=admin
INFLUXDB_USER=proxmox
INFLUXDB_USER_PASSWORD=proxmox
INFLUXDB_URL=http://influxdb:8086

# Proxmox configuration
PROXMOX_HOST=proxmox.local
# User format should be username@realm (e.g., root@pam or monitoring@pve)
PROXMOX_USER=root@pam
# API Token ID that you created in Proxmox UI (e.g., monitoring)
PROXMOX_TOKEN_ID=your_token_id
# API Token Secret - the long string generated when you created the token
PROXMOX_TOKEN_SECRET=your_token_secret 